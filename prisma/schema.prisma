datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Play {
    id        String   @id @default(uuid())
    title     String
    content   Json
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
